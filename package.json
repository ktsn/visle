{
  "name": "visle",
  "version": "0.2.7",
  "description": "Islands Architecture renderer for Vue.js",
  "keywords": [
    "Vue.js",
    "islands architecture",
    "multi-page application",
    "server side rendering",
    "template engine"
  ],
  "license": "MIT",
  "author": "Katashin",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/server/index.js",
    "./build": "./dist/build/index.js",
    "./dev": "./dist/server/dev.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "test": "vitest",
    "test:coverage": "vitest --coverage --coverage.reporter=lcov",
    "lint": "oxlint",
    "format": "oxfmt --write",
    "format:check": "oxfmt --check",
    "prepublishOnly": "pnpm lint && pnpm run test --run && rm -rf dist && pnpm build"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "connect": "^3.7.0",
    "glob": "^13.0.0",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@types/connect": "^3.4.38",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.9",
    "@types/sanitize-html": "^2.16.0",
    "@vitest/coverage-v8": "4.0.18",
    "oxfmt": "^0.32.0",
    "oxlint": "^1.42.0",
    "rollup": "^4.55.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "vue": "^3.5.27"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc"
}
