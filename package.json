{
  "name": "vue-islands-renderer",
  "version": "0.1.0",
  "description": "Islands Architecture renderer for Vue.js",
  "type": "module",
  "exports": {
    "./build": "./dist/build/index.js",
    "./server": "./dist/server/index.js",
    "./client": "./dist/client/custom-element.js"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p .",
    "test": "vitest",
    "lint": "pnpm biome lint ./",
    "format": "pnpm biome format --write ./",
    "demo": "cd demo && node --experimental-strip-types dev.ts",
    "demo:production": "cd demo && node --experimental-strip-types prod.ts",
    "demo:build": "cd demo && vite build && vite build --ssr src/server.ts",
    "prepublishOnly": "pnpm lint && pnpm run test --run && rm -rf dist && pnpm build"
  },
  "keywords": [
    "Vue.js",
    "islands architecture",
    "template engine",
    "server side rendering",
    "multi-page application"
  ],
  "author": "Katashin",
  "license": "MIT",
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/express": "^5.0.0",
    "@types/node": "^22.5.4",
    "@types/sanitize-html": "^2.13.0",
    "@vitest/coverage-v8": "2.1.3",
    "express": "^4.21.0",
    "marked": "^14.1.2",
    "rollup": "^4.21.2",
    "sanitize-html": "^2.13.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitest": "^2.1.3",
    "vue": "^3.5.9"
  },
  "peerDependencies": {
    "vite": ">=5.0.0",
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.2.0",
    "glob": "^11.0.0"
  }
}
