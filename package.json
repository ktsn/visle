    "@babel/types": "^7.29.0",
    "@types/connect": "^3.4.38",
    "@types/diffable-html": "^5.0.2",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.9",
    "@types/sanitize-html": "^2.16.0",
    "@vitest/coverage-v8": "4.0.18",
    "diffable-html": "^6.0.1",
    "happy-dom": "^20.7.0",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.42.0",
    "playwright": "^1.58.2",
    "rollup": "^4.55.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "vue": "^3.5.27"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
